<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string" />
    <attr name="alignTextStart" format="boolean" />

    <declare-styleable name="ClickableLayout">
        <attr name="text" />
        <attr name="drawableEnd" />
        <attr name="alignTextStart" />
    </declare-styleable>

    <declare-styleable name="DrawableClickableLayout">
        <attr name="text" />
        <attr name="drawableStart" />
        <attr name="drawableEnd" />
        <attr name="alignTextStart" />
    </declare-styleable>

    <declare-styleable name="ListNavigatorView">
        <attr name="textSize" format="dimension" />
        <attr name="textSizePressed" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textColorPressed" format="color" />
        <attr name="textSpanScale" format="float" />
        <attr name="background" />
        <attr name="backgroundPressed" format="color" />
    </declare-styleable>

    <declare-styleable name="ClearableEditText">
        <attr name="drawableStart" />
        <attr name="drawableStartVisible" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="drawableEnd" />
    </declare-styleable>
</resources>