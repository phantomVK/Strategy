<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClickableLayout">
        <attr name="text" />
        <attr name="drawableStart" />
        <attr name="drawableEnd" />
        <attr name="alignTextStart" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ListNavigatorView">
        <attr name="textSize" />
        <attr name="textSizePressed" format="dimension" />
        <attr name="textColor" />
        <attr name="textColorPressed" format="color" />
        <attr name="textSpanScale" format="float" />
        <attr name="background" />
        <attr name="backgroundPressed" format="color" />
    </declare-styleable>
</resources>
